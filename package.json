{
  "name": "linea-token-rescue",
  "version": "1.0.0",
  "type": "module",
  "description": "Web app to rescue LINEA tokens from compromised wallets using atomic bundle transactions",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "generate-merkle": "tsx scripts/generate-merkle-tree.ts"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "ethers": "^6.8.1",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.20.3",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
